class Solution {
public:
    int minMoves(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int x=nums[0];
        int res=0;
        int n=nums.size();
        int y=nums[n-1];
        for(int i=0;i<n;i++){
            res+=abs(nums[i]-x);
        }return res;
    }
};//ANIKET CODESS...
