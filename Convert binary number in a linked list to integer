class Solution {
public:
void fast()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
}
    int getDecimalValue(ListNode* head) {
        fast();
       ListNode* temp=head;
       int num=0;
       while(temp!=NULL){
           num=num*2+(temp->val);
           temp=temp->next;
       }
       return num;
    }
};
